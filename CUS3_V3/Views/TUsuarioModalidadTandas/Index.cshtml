@model IEnumerable<CUS3_V3.Models.TUsuarioModalidadTanda>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IumtPuntajeTotal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IumtPista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkIumtCodPartNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkIumtCodTanNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IumtPuntajeTotal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IumtPista)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkIumtCodPartNavigation.VmUmArchivoB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkIumtCodTanNavigation.VtDescripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkVumtCod">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PkVumtCod">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PkVumtCod">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
